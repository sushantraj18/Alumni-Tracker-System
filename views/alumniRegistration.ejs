        <!-- About Start -->
        <form action="/alumni/alumniRegistration" method="post" enctype="multipart/form-data" class="form-group">
        <div class="about wow fadeInUp" data-wow-delay="0.1s">
            <div class="container">
                <div class="row align-items-center">
                    <div class="section-header text-center">
                        <h4>Alumni Registration <small>
                                <span style="color:red">
                                    <%=message%>
                                </span>
                            </small> </h4>

                    </div>
                    <div class="col-lg-6 col-md-6">
                            <input type="text" class="form-control" placeholder="Enter User Name" name="userName"
                                id="userName" />

                            
                            <input type="email" class="form-control" placeholder="Enter Email" name="email" id="email"  />

                            
                            <input type="password" placeholder="Enter password" class="form-control" name="password" id="password" />

                            <input type="text" placeholder="Enter mobile number" class="form-control" name="contact" id="contact" />

                            Date of Birth
                            <input type="date" placeholder="DOB" class="form-control" name="dateOfBirth" id="dateOfBirth" />


                            Select Gender
                            <input type="radio" value="male" name="gender"
                            id="male" /> <label for="male">Male</label>
                            <input type="radio" value="female" name="gender" id="female"/> <label for="female">Female</label>

                            


                        <input type="number" class="form-control" placeholder="Enter pass out year" name="passOutYear"
                        id="passOutYear" />



                        Select Stream

                        <select class="form-control" name="stream" id="stream">
                            <option value="">Select Stream</option>
                            <option value="Computer Science">Computer Science</option>
                            <option value="Information Technology">Information Technology</option>
                            <option value="Electronics">Electronics</option>
                            <option value="Mechanical">Mechanical</option>
                            <option value="Agriculture">Agriculture</option>
                            <option value="Business Administrative">Business Administrative</option>
                            <option value="Business Administrative">Business Administrative</option>
                            <option value="Finance">Finance</option>
                            <option value="HR">HR</option>
                        </select>



                    </div>
                    <div class="col-lg-6 col-md-6 ">


                            Select Branch
                        <select class="form-control" name="branch" id="branch">
                            <option value="">Select Branch</option>
                            <option value="BTech">BTech</option>
                            <option value="MTech">MTech</option>
                            <option value="BCA">BCA</option>
                            <option value="MCA">MCA</option>
                            <option value="BSC">BSC</option>        
                            <option value="MSC">MSC</option>
                            <option value="MBA">MBA</option>
                            <option value="BBA">BBA</option>
                        </select>


                        Select Experience
                        <select class="form-control" name="experience" id="experience">
                            <option value="">Select Experience</option>
                            <option value="6 Month">6 Month</option>
                            <option value="1+ Year">1+ Year</option>
                            <option value="2+ Year">2+ Year</option>
                            <option value="4+ Year">4+ Year</option>
                            <option value="6+ Year">6+ Year</option>
                            <option value="8+ Year">8+ Year</option>
                            <option value="10+ Year">10+ Year</option>
                            
                        </select>

                        <input type="text" class="form-control" placeholder="Enter Current Company" name="currentCompany"
                            id="currentCompany" />

                        <input type="text" class="form-control" placeholder="Enter Designation" name="designation"
                            id="designation" />


                            
                        Upload Latest Profile Pic
                        <input type="file" class="form-control"  name="profile"
                            id="profile" />

                        LinkedIn Profile Link
                        <input type="url" class="form-control" placeholder="Enter LinkedIn Profile Link "  name="linkedInProfileLink"
                            id="linkedInProfileLink" />

                        <input class="btn btn-warning btn-block" type="submit" value="Add Alumni" />

                        <input class="btn btn-danger btn-block" type="reset" value="Reset" />
                        
                    </div>
                </div>
            </div>
        </div>
    </form>
        <!-- About End -->



