        <!-- About Start -->
        <form action="/admin/adminAddEvent" method="post" class="form-group">
        <div class="about wow fadeInUp" data-wow-delay="0.1s">
            <div class="container">
                <div class="row align-items-center">
                    <div class="section-header text-center">
                        <h4>Admin add Event <small>
                                <span style="color:red">
                                    <%=message%>
                                </span>
                            </small> </h4>

                    </div>
                    <div class="col-lg-6 col-md-6">
                            <input type="text" class="form-control" placeholder="Enter Event Name" name="eventName"
                                id="eventName" />

                            Event Start Date
                            <input type="date" class="form-control" name="eventStartDate" id="eventStartDate" />

                            Event End Date
                            <input type="date" class="form-control" name="eventEndDate" id="eventEndDate" />

                            <input type="text" placeholder="Event Start Time" class="form-control" name="eventStartTime" id="eventstartTime" />

                            
                            <input type="text" placeholder="Event End Time" class="form-control" name="eventEndTime" id="eventEndTime" />


                            <input type="text" class="form-control" name="location" placeholder="Enter Location"
                                id="location" />

                            Event Description
                            <textarea class="form-control" name="description" id="description"></textarea>


                    </div>
                    <div class="col-lg-6 col-md-6 ">



                        <input type="text" class="form-control" placeholder="Type of Event" name="typeOfEvent"
                            id="typeOfEvent" />

                        <input type="text" class="form-control" placeholder="Enter Criteria" name="criteria"
                            id="criteria" />

                        Mode of Apply (optional)
                        <select class="form-control" name="modeOfApply" id="modeOfApply">
                            <option value="">Select Mode of Apply</option>
                            <option value="Online">Online</option>
                            <option value="Ofline">Ofline</option>
                            <option value="Both">Both</option>
                        </select>

                        Start Date to Apply (optional)
                        <input type="date" class="form-control" name="startDateToApply" id="startDateToApply" />

                        Last Date to Apply (optional)
                        <input type="date" class="form-control" name="lastDateToApply" id="lastDateToApply" />

                        <input class="btn btn-warning btn-block" type="submit" value="Add Event" />

                        <input class="btn btn-danger btn-block" type="reset" value="Reset" />
                        
                    </div>
                </div>
            </div>
        </div>
    </form>
